<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.tomtest.Context</listener-class>
  </listener>
  <servlet>
  	<servlet-name>FetchData</servlet-name>
  	<display-name>FetchData</display-name>
  	<description></description>
  	<servlet-class>com.tomtest.FetchData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertData</servlet-name>
  	<display-name>InsertData</display-name>
  	<description></description>
  	<servlet-class>com.tomtest.InsertData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteData</servlet-name>
  	<display-name>DeleteData</display-name>
  	<description></description>
  	<servlet-class>com.tomtest.DeleteData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FetchData</servlet-name>
  	<url-pattern>/FetchData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertData</servlet-name>
  	<url-pattern>/InsertData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteData</servlet-name>
  	<url-pattern>/DeleteData</url-pattern>
  </servlet-mapping>
</web-app>
